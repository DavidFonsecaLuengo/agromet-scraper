              <script>
              function initialize() {
                  /* ---> ELEMENTO CONTENEDOR <--- */
                  var obj = document.getElementById("map_canvas");

                  /* ---> THIS IS THE LOCATION FOR THE PIN <--- */
                  var pin = new google.maps.LatLng(-40.52036, -73.06592);

                  var center = new google.maps.LatLng(-40.16, -72.63);                  
                  //var center = new google.maps.LatLng(-30.7232, -70.7726);/*rapel*/

                  /* ---> VARIABLES GENERALES <--- */
                  var regiones    = new Array() ;
                  var estaciones  = new Array() ;
                  var ubicacionR  = new Array() ;
                  var ubicacionE  = new Array() ;
                  var texto       = new Array() ;
                  var markers     = new Array() ;


                  var styles = [
                         {
                            featureType: "water",
                            elementType: "geometry.fill",
                            stylers: [
                              { color: "#828280" },
                              { hue: "#00c3ff" },
                              { saturation: 90 },
                              { lightness: 90 }
                            ]
                          }
                      ];

                  var mapOptions = {
                    zoom: 7,
                    center: center,
                    mapTypeId: google.maps.MapTypeId.TERRAIN,
                    panControl: false,
                    scrollwheel: false,
                    draggable: true,
                    zoomControlOptions: {
                        style: google.maps.ZoomControlStyle.BIG,
                        position: google.maps.ControlPosition.LEFT_TOP
                    },
                    streetViewControl: false,
                    styles: styles
                  }

                  var map = new google.maps.Map(obj, mapOptions);
            
                  var burbuja = new google.maps.InfoWindow();
  
                  google.maps.event.addListener(map, 'click', function(){
                      cerrarBurbuja();
                  });

              

                  map.setTilt(0);

                  regiones[0]='Arica y Parinacota - XV';
