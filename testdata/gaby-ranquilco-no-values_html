        <!-- Load scripts ================= -->
        <script src="./gaby-ranquilco-no-values_html_files/jquery.min.js"></script>
        <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.7.0.min.js"><\/script>')</script>
        <script src="./gaby-ranquilco-no-values_html_files/plugins.js"></script>
        <script src="./gaby-ranquilco-no-values_html_files/main.js"></script>
                <script type="text/javascript">
          $('html,body').animate({
              scrollTop: $("#marca").offset().top
            }, 1000);
        </script>
        
        <!-- Select -->        
        <script type="text/javascript" src="./gaby-ranquilco-no-values_html_files/chosen.jquery.min.js"></script>
        <!-- Calendario -->
        <script type="text/javascript" src="./gaby-ranquilco-no-values_html_files/jquery-ui.js"></script>

        <script type="text/javascript">

        $(document).ready(function(){
            initialize();
            $(".chzn-select").chosen({no_results_text: "No se encontraron resultados"});

            colors = ['#EF8D1A', '#FFFFFF'];
            var i = 0;
            animate_loop = function(objeto) {      
            $('#'+ objeto).animate({backgroundColor:'#EF8D1A'
              }, 500);
            $('#'+ objeto).animate({backgroundColor:'#f9f9f9'
              }, 500);
            $('#'+ objeto).animate({backgroundColor:'#EF8D1A'
              }, 500);
            $('#'+ objeto).animate({backgroundColor:'#f9f9f9'
              }, 500);
            }
            
        });


        function seleccionarEma(elemento) {
           var combo = document.forms["consulta"].estaciones;
           var cantidad = combo.length;
           for (i = 0; i < cantidad; i++) {
              if (combo[i].value == elemento) {
                 combo[i].selected = 'true';
                 $("#estaciones").trigger("liszt:updated");
              }   
           }

           $('html,body').animate({
              scrollTop: $("#consulta").offset().top
            }, 500);

            animate_loop('ema');


          }

        function setTipoCalendario(obj){
          var intervalo = obj.value;
          if(intervalo=="hour" || intervalo=="day"){
            $('#tr_horadia').show();
            $('#tr_mes, #tr_ano').hide();
          }
          else if(intervalo=="month"){
            $('#tr_mes').show();
            $('#tr_horadia, #tr_ano').hide();
          }
          else if(intervalo=="year"){
            $('#tr_ano').show();
            $('#tr_horadia, #tr_mes').hide();
          }
        }


        function validar(){
          var alertas = new Array() ;
          var errores = new Array() ;
          var alerta_ms="";
          var error_ms="";
          var opciones = new Array();
          var indice=0;
          var sum_ema=0;
          var sum_var=0;
          var intervalo=$('#intervalos').val();
          var vars = $('#variables option:selected');
          var values = $.map(vars ,function(option) {
              return option.value;
          });

          $('select#estaciones option:selected').each(function(){
              sum_ema += 1; 
          });

          $('select#variables option:selected').each(function(){
              sum_var += 1; 
          });


          if(sum_ema==0){
              alertas[0]="estación";
          }

          if(sum_var==0){
              alertas[1]="variable";
          }

          if(intervalo=="hour" || intervalo=="day"){
            if($('#desde').val()==""){
              alertas[2]="desde";  
            }
            else{
              if(intervalo=="hour"){
                $("#variables option:selected").each(function()
                {
                    if($(this).val() == '30000-variable'){
                      alertas[2]="ET0 : intervalo no válido.";
                    }
                    if($(this).val() == '17-formula'){
                      alertas[3]="GD(5) : intervalo no válido.";
                    }
                    if($(this).val() == '18-formula'){
                      alertas[4]="GD(10): intervalo no válido.";
                    } 
                    if($(this).val() == '11-formula'){
                      alertas[5]="Horas Frío: intervalo no válido.";
                    }
                    if($(this).val() == '27-formula'){
                      alertas[6]="GF(7): intervalo no válido.";
                    }  
                });

              }
              else if(intervalo=="day"){
                $("#variables option:selected").each(function()
                {
                    if($(this).val() == '29-formula'){
                      alertas[6]="IET: intervalo no válido (sólo hora).";
                    } 
                });
              }
            }

            if($('#hasta').val()==""){
              alertas[3]="hasta";  
            }
          }
          else if(intervalo=="month"){
            if( (new Date("01-"+ $('#desde_meses').val() + "-" + $('#desde_anos').val()).getTime() > new Date("01-"+ $('#hasta_meses').val() + "-" + $('#hasta_anos').val()).getTime())){
              errores[0]="fecha inicial no puede ser mayor a fecha final"; 
            }
            else{
              $("#variables option:selected").each(function()
                {
                    if($(this).val() == '29-formula'){
                      alertas[6]="IET: intervalo no válido (sólo hora).";
                    } 
                });
            } 
          }
          else if(intervalo=="year"){
            if( (new Date("01-01-"+ $('#desde_anos_a').val()).getTime() > new Date("01-01-"+ $('#hasta_anos_a').val()).getTime())){
              errores[0]="año inicial no puede ser mayor al año final"; 
            } 
            else{
              $("#variables option:selected").each(function()
                {
                    if($(this).val() == '30000-variable'){
                      alertas[2]="ET0 : intervalo no válido.";
                    }
                    if($(this).val() == '17-formula'){
                      alertas[3]="GD(5) : intervalo no válido.";
                    }
                    if($(this).val() == '18-formula'){
                      alertas[4]="GD(10): intervalo no válido.";
                    } 
                    if($(this).val() == '27-formula'){
                      alertas[5]="GF(7): intervalo no válido.";
                    } 
                    if($(this).val() == '29-formula'){
                      alertas[6]="IET: intervalo no válido (sólo hora).";
                    } 
                });
            }

          }
          /*-----------FIN DESDE HASTA MESES--------------------*/



          /*-------OPCIONES----------*/
          /*$('[name=opciones]').each(function(){
              if( $(this).attr('checked') ){                           
                  opciones[indice] = $(this).val();//El indice inicial cera 0
                      indice++; //paso a incrementar el indice en 1
              }
          });

          if(opciones==""){
            alertas[4]="opciones de vista";  
          }*/

          if(!$('#html').attr('checked') && !$('#excel').attr('checked') && !$('#grafica').attr('checked')){
            alertas[4]="opciones de vista";  
          }

          /*-------FIN OPCIONES----------*/

          $(alertas).each(function(i,alerta){
            if(alerta!=undefined){
              alerta_ms += "<li>"+ alerta + "</li>";
            }
          });  

          $(errores).each(function(i,error){
            if(error!=undefined){
              error_ms += "["+ error + "] ";
            }
          });     
          
            
          if(alerta_ms || error_ms){
            if(alerta_ms){
              $('#alerta span').html("<strong>Complete los campos: </strong><ul class='campos'>" + alerta_ms + "</ul>");
              $('#alerta').show();
            }
            if(error_ms){
              $('#error span').html("<strong>Error: </strong>" + error_ms);
              $('#error').show();
            }
          }  
          else{
              if((sum_ema>1 && sum_var>1) || (sum_ema==1 && sum_var>5) || (sum_ema>5)){
                if(sum_ema>1 && sum_var>1){
                  $('#alerta span').html("Si selecciona más de <strong>una estación</strong>, usted sólo podrá consultar <strong>una variable</strong>.");
                }
                else if(sum_ema==1 && sum_var>5){
                  $('#alerta span').html("Usted puede seleccionar un máximo de <strong>5 variables</strong> para consultar al mismo tiempo.");
                }
                else if(sum_ema>5 && sum_var==1){
                  $('#alerta span').html("Usted puede seleccionar un máximo de <strong>5 estaciones</strong> para consultar al mismo tiempo.");
                }


                
                $('#alerta').show();
              }
              else{
                document.consulta.submit();
              } 
          }
        }

        function mostrarCapa(layer){
            $(document).ready(function()
            {
                if(layer==1)
                {
                    $('.ayer').hide();
                    $('.hoy').show('slow');
                }
                else if(layer==2)
                {
                    $('.hoy').hide();
                    $('.ayer').show('slow');
                }
            });
        }

        /*----Calendario----*/
        $(function() {
            $( "#desde" ).datepicker({
                defaultDate: "+1w",
                changeMonth: true,
                changeYear: true,
                numberOfMonths: 1,
                dateFormat: 'dd-mm-yy',
                closeText: 'Cerrar',
                prevText: '<Ant',
                nextText: 'Sig>',
                currentText: 'Hoy',
                monthNames: ['Enero', 'Febrero', 'Marzo', 'Abril', 'Mayo', 'Junio', 'Julio', 'Agosto', 'Septiembre', 'Octubre', 'Noviembre', 'Diciembre'],
                monthNamesShort: ['Ene','Feb','Mar','Abr', 'May','Jun','Jul','Ago','Sep', 'Oct','Nov','Dic'],
                dayNames: ['Domingo', 'Lunes', 'Martes', 'Miércoles', 'Jueves', 'Viernes', 'Sábado'],
                dayNamesShort: ['Dom','Lun','Mar','Mié','Juv','Vie','Sáb'],
                dayNamesMin: ['Do','Lu','Ma','Mi','Ju','Vi','Sá'],
                weekHeader: 'Sm',
                firstDay: 1,
                yearRange: "2009:2018",
                maxDate:"14-01-2018",
                onClose: function( selectedDate ) {
                    $( "#hasta" ).datepicker( "option", "minDate", selectedDate );
                }
            });
            $( "#hasta" ).datepicker({
                defaultDate: "+1w",
                changeMonth: true,
                changeYear: true,
                numberOfMonths: 1,
                dateFormat: 'dd-mm-yy',
                closeText: 'Cerrar',
                prevText: '<Ant',
                nextText: 'Sig>',
                currentText: 'Hoy',
                monthNames: ['Enero', 'Febrero', 'Marzo', 'Abril', 'Mayo', 'Junio', 'Julio', 'Agosto', 'Septiembre', 'Octubre', 'Noviembre', 'Diciembre'],
                monthNamesShort: ['Ene','Feb','Mar','Abr', 'May','Jun','Jul','Ago','Sep', 'Oct','Nov','Dic'],
                dayNames: ['Domingo', 'Lunes', 'Martes', 'Miércoles', 'Jueves', 'Viernes', 'Sábado'],
                dayNamesShort: ['Dom','Lun','Mar','Mié','Juv','Vie','Sáb'],
                dayNamesMin: ['Do','Lu','Ma','Mi','Ju','Vi','Sá'],
                weekHeader: 'Sm',
                firstDay: 1,
                yearRange: "2009:2018",
                maxDate:"14-01-2018",
                onClose: function( selectedDate ) {
                    $( "#desde" ).datepicker( "option", "maxDate", selectedDate );
                }
            });
        });
        /*----Fin Calendario----*/


        function moverAconsulta(){
          $('html,body').animate({
              scrollTop: $("#consulta").offset().top
            }, 500);
        }


        /*GRAFICOS*/
