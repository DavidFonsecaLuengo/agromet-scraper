                  $("div.cargando").hide(); 

                  //Lista regiones
                  $(ubicacionR).each(function(i,marker){
                    /*Menu superior*/
                    $("<li>").html($("<a href='#estaciones' title='centrar mapa en " +regiones[i]+"'>").html(regiones[i])).click(function(){mostrarRegion(marker, false);}).appendTo(".regionesMnuTop");
                    google.maps.event.addListener(marker, "click", function(){
                      mostrarRegion(marker, false);
                    });

                  });

                  //Lista estaciones
                  $(ubicacionE).each(function(i,marker){                    
                      google.maps.event.addListener(marker, "click", function(){
                          mostrarEstacion(marker, texto[i]);
                      });
                  }); 


                  function mostrarRegion(marker, mover){ 
                      map.panTo(marker.getPosition());//centra el mapa
                      cerrarBurbuja();
                      if(mover){
                        $('html,body').animate({
                          scrollTop: $("#map_canvas").offset().top
                        }, 500);
                      }
                  }

                  function mostrarEstacion(marker, texto){                         
                      var markerLatLng = marker.getPosition();
                      burbuja.setContent([texto].join(''));
                      burbuja.open(map, marker);
                  }


                  function cerrarBurbuja() {
                      burbuja.close();
                  }

                  google.maps.event.addListener(map, 'zoom_changed', function() {
                    cerrarBurbuja()
                  });

                  google.maps.event.addListener(burbuja, 'domready', function(event) {
                    
                          !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];

                          /*if(!d.getElementById(id)){*/
                            js=d.createElement(s);js.id=id;
                            js.src="//platform.twitter.com/widgets.js";
                            fjs.parentNode.insertBefore(js,fjs);
                          //}

                        }(document,"script","twitter-wjs");
                  });
                  
                  var mcOptions = {
                    gridSize: 50, 
                    maxZoom: 6
                  };               
                  var markerCluster = new MarkerClusterer(map, markers, mcOptions);

                  /*fin initialize*/
              }

              
              </script>

